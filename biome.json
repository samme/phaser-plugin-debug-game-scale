{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"formatter": {
		"enabled": false
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"complexity": {
				"useArrowFunction": "off"
			},
			"suspicious": {
				"noAssignInExpressions": "off",
				"noPrototypeBuiltins": "off"
			}
		}
	}
}
